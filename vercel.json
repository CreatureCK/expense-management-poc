{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "TABSCANNER_API_KEY": "@tabscanner_api_key",
      "OPENAI_API_KEY": "@openai_api_key"
    }
  }
